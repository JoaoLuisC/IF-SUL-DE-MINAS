#include <stdio.h>

int pot(int base,int expo){
	
	int i, res=1;
	
	for(i=1;i<=expo;i++){
		
		res = res*base;
		
	}
	
	return res;
	
}


int main(){
	
	int res,base,expo;
	
	printf("escreva a base da potencia:");	
	scanf("%d",&base);
	
	printf("escreva o expoente da potencia:");	
	scanf("%d",&expo);
	
	res = pot(base,expo);
	
	printf("res = %d",res);
	
}
